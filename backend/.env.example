
#############################################
# PRODUCTION ENVIRONMENT (.env example)
#############################################

# TYPEORM / MSSQL CONFIG
MSSQL_SERVER=your_production_server
MSSQL_PORT=1433
MSSQL_INSTANCE=your_instance_if_any
MSSQL_USERNAME=your_production_username
MSSQL_PASSWORD=your_production_password
MSSQL_DATABASE=your_production_database

# For schema-check endpoint or other tools
MSSQL_URL=Server=your_production_server,1433;Database=your_production_database;User Id=your_production_username;Password=your_production_password;Encrypt=true;TrustServerCertificate=true

# CORS Configuration
CORS_ORIGIN=https://your-production-frontend-url

# Frontend origin for CORS
FRONTEND_ORIGIN=https://your-production-frontend-url

# Server Configuration
PORT=3000
NODE_ENV=production

# Logging
LOG_LEVEL=info

# Auth0 Configuration
AUTH0_DOMAIN=your-auth0-domain
AUTH0_AUDIENCE=your-auth0-audience
AUTH0_ISSUER=your-auth0-issuer

#############################################
# LOCAL DEVELOPMENT ENVIRONMENT (.env.local example)
#############################################

# TYPEORM / MSSQL CONFIG
MSSQL_SERVER=localhost\SQLEXPRESS
MSSQL_PORT=1433
MSSQL_INSTANCE=SQLEXPRESS
MSSQL_USERNAME=your_local_username
MSSQL_PASSWORD=your_local_password
MSSQL_DATABASE=your_local_database

# For schema-check endpoint or other tools
MSSQL_URL=Server=localhost\SQLEXPRESS,1433;Database=your_local_database;User Id=your_local_username;Password=your_local_password;Encrypt=true;TrustServerCertificate=true

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Frontend origin for CORS
FRONTEND_ORIGIN=http://localhost:5173

# Server Configuration
PORT=3000
NODE_ENV=development

# Logging
LOG_LEVEL=debug

# Auth0 Configuration
AUTH0_DOMAIN=your-auth0-domain
AUTH0_AUDIENCE=your-auth0-audience
AUTH0_ISSUER=your-auth0-issuer

